{
  "name": "multilang-per-files-frontend",
  "version": "1.0.0",
  "description": "Frontend demonstrating per-language proto files with smart exclusions",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist src/proto/generated"
  },
  "keywords": [
    "protobuf",
    "typescript",
    "bufrnix",
    "per-language-files",
    "google-api-annotations",
    "connect-es"
  ],
  "author": "Bufrnix Example",
  "license": "MIT",
  "dependencies": {
    "@bufbuild/protobuf": "^2.6.3",
    "@connectrpc/connect": "^1.4.0",
    "@connectrpc/connect-web": "^1.4.0"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "tsx": "^4.20.4",
    "@types/node": "^20.0.0"
  }
}