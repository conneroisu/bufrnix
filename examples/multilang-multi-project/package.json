{
  "name": "multilang-multi-project-example",
  "version": "1.0.0",
  "description": "JavaScript/TypeScript example for multi-language multi-project protobuf usage",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "dev": "node --watch index.js",
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "keywords": [
    "protobuf",
    "grpc",
    "multi-language",
    "bufrnix",
    "example"
  ],
  "author": "Bufrnix Example",
  "license": "MIT",
  "dependencies": {
    "google-protobuf": "^3.21.2",
    "@grpc/grpc-js": "^1.9.0",
    "@grpc/proto-loader": "^0.7.0"
  },
  "devDependencies": {
    "@types/google-protobuf": "^3.15.6",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}